doctype transitional
html(xmlns='http://www.w3.org/1999/xhtml')
  head
    style(type='text/css' rel='stylesheet' media='all').
      /* General */
      body {
        width: 100%;
        height: 100%;
        margin: 0;
        padding: 0;
        font-family: "GorditaRegular", sans-serif;
        font-size: 14px;
        font-weight: 400;
        color: #212121;
      }
      * {
        box-sizing: border-box;
        -moz-box-sizing: border-box;
      }
      tr {
        margin: 0;
        padding: 0;
      }
      td {
        margin: 0;
        padding: 0;
      }
      /* Typography */
      .logo{
        font-family: "GorditaBold", sans-serif;
        font-weight: 700;
        font-size: 20px;
        line-height: 2.36;
        color: #1E1E1E;
        text-decoration: none;
      }
      .logo-sub{
        font-family: "GorditaRegular", sans-serif;
        font-weight: 300;
        font-size: 16px;
        color: #888;
        text-decoration: none;
      }
      .title-1{ }
      .txt-l{ text-align: left; }
      .txt-c{ text-align: center; }
      .txt-j{ text-align: justify; }
      .txt-r{ text-align: right; }
      /* Grid */
      .grid{
        /*display: flex;*/
      }
      .grid .col{  }
      .grid .col.w-1-3{ width: 33.33%; }
      .grid .col.w-2-3{ width: 66.66%; }
      .grid .col.w-3-3{ width: 100%; }
      .grid .col .col-inner{ padding: 0 10px; }
      .grid .col:nth-child(1) .col-inner{
        padding-left: 0;
      }
      .grid .col:last-child .col-inner{
        padding-right: 0;
      }
      /* Box */
      .box{
        /*border: 1px solid #D7D7D7;*/
        /*border-radius: 4px;*/
        /*padding: 0 20px 0 30px;*/
        /*border-left: 3px solid #D7D7D7;*/
      }
      .box-head{
        position: relative;
      }
      .box-head .icon{
        position: relative;
        width: 48px;
        height: 48px;
        border: 2px solid #212121;
        border-radius: 100%;
        padding: 5px;
      }
      .box-head .line{
        position: absolute;
        top: 24px;
        left: 48px;
        height: 2px;
        width: 100%;
        background-color: #212121;
      }
      .box-title{
        position: absolute;
        top: 5px;
        left: 58px;
        font-family: "GorditaMedium", sans-serif;
        font-size: 20px;
        line-height: 26px;
        text-align: left;
        color: #212121;
        background-color: #FFFFFF;
        padding: 5px 10px;
      }
      .box-body{
        margin: 30px 0 0 0;
        padding-left: 70px;
      }
      .box-row{
        position: relative;
        margin-bottom: 30px;
      }
      .date-holder {
        height: auto;
        min-height: 100%;
        min-width: 63px;
        position: absolute;
        top: 0;
        left: 0;
      }
      .date-start{
        position: absolute;
        top: 0;
        left: 0;
      }
      .date-year {
        font-family: "GorditaMedium", sans-serif;
        font-size: 16px;
        line-height: 21px;
        text-align: left;
        color: #888;
      }
      .date-month {
        font-family: "GorditaMedium", sans-serif;
        font-size: 12px;
        line-height: 16px;
        text-align: left;
        color: #888888;
      }
      .date-separator {
        width: 1px;
        background-color: #e6e6e6;
        position: absolute;
        top: 60px;
        bottom: 60px;
        left: 40%;
      }
      .date-end{
        position: absolute;
        bottom: 0;
        left: 0;
      }
      .content-holder {
        display: inline-block;
        padding-left: 80px;
      }
      .title-holder{}
      .title-holder .title {
        font-family: "GorditaMedium", sans-serif;
        font-size: 14px;
        font-weight: bold;
        line-height: 19px;
        text-align: left;
        color: #212121;
        margin-bottom: 2px;
      }
      .title-holder .subtitle {
        font-family: "GorditaRegular", sans-serif;
        font-size: 12px;
        line-height: 16px;
        text-align: left;
        color: #888;
        margin-bottom: 2px;
      }
      .details-holder {
        font-family: "GorditaRegular", sans-serif;
        font-size: 14px;
        line-height: 21px;
        text-align: left;
        color: #212121;
        margin-bottom: 5px;
      }
      .details-holder ul {
        padding: 20px 0 0 20px;
        margin: 0;
      }
      .details-holder ul li {
        font-family: "GorditaRegular", sans-serif;
        font-size: 14px;
        text-align: left;
        color: #212121;
      }
      /* Page styles */
      .page {}
      .page-inner {}
      .page-head{
        margin-bottom: 30px;
      }
      .page-body{}
      /* Personal info Box */
      .personal-info{  }
      .personal-info .icon { margin-right: 4px; }
      .profile-img-holder {
        margin-bottom: 32px;
      }
      .profile-img{
        width: 96px;
        height: 96px;
        margin: 0 auto;
        border-radius: 100%;
      }
      .user-fullname {
        font-family: "GorditaMedium", sans-serif;
        font-size: 16px;
        font-weight: bold;
        color: #212121;
        margin-bottom: 13px;
      }
      .user-title {
        font-family: "GorditaMedium", sans-serif;
        font-size: 14px;
        color: #212121;
        margin-bottom: 8px;
      }
      .user-subtitle {
        font-family: "GorditaRegular", sans-serif;
        font-size: 12px;
        color: #444;
        margin-bottom: 13px;
      }
      .user-address {
        font-size: 13px;
        color: #666;
      }
      .user-worktime {
        font-size: 13px;
        color: #666;
      }
      .user-worktime > *, .user-address > * {
        display: inline-block;
        vertical-align: middle;
      }
      /* Skills box */
      .tbl-skills{
        width: 100%;
        border: none;
      }
      .tbl-skills tr td{
        vertical-align: top;
      }
      .tbl-skills tr td:nth-child(1){
        color: #888;
      }
      table.skills-table {
        width: 100%;
        border: none;
      }
      table.skills-table thead tr th {
        font-family: "GorditaMedium", sans-serif;
        font-size: 12px;
        color: rgba(0,0,0,.5);
        text-align: left;
        line-height: 16px;
        padding-bottom: 16px;
        border-bottom: 1px solid #e8e8e8;
      }
      table.skills-table thead tr th:nth-child(2) { text-align: right; }
      table.skills-table tbody tr td {
        font-family: "GorditaRegular", sans-serif;
        font-size: 14px;
        color: #212121;
        text-align: left;
        line-height: 22px;
        border-bottom: 1px solid #e8e8e8;
        padding: 8px 0;
      }
      table.skills-table tbody tr td:nth-child(2) { text-align: right; }
      /* Divider */
      .border-divider{
        position: relative;
        padding: 40px;
      }
      .border-divider .border-bg{
        display: block;
        position: absolute;
        top: 50%;
        height: 3px;
      }
      .border-divider .border-bg.left{ left: 0; }
      .border-divider .border-bg.right{ right: 0; }
      .border-divider .border-bg.grey-light{ background-color: #D7D7D7; }
      .border-divider .border-bg.black{ background-color: #D7D7D7; }
      /* Helpers */
      .inline-block{ display: inline-block; }
      .float-left{ float: left; }
      .float-right{ float: right; }
      .w-10{ width: 10%; }
      .w-20{ width: 20%; }
      .w-30{ width: 30%; }
      .w-40{ width: 40%; }
      .w-50{ width: 50%; }
      .w-60{ width: 60%; }
      .w-70{ width: 70%; }
      .w-80{ width: 80%; }
      .w-90{ width: 90%; }
      .w-100{ width: 100%; }
      .margin-t-10{ margin-top: 10px; }
      .margin-t-20{ margin-top: 20px; }
      .margin-t-30{ margin-top: 30px; }
      .margin-t-40{ margin-top: 40px; }
      .margin-b-10{ margin-bottom: 10px; }
      .margin-b-20{ margin-bottom: 20px; }
      .margin-b-30{ margin-bottom: 30px; }
      .margin-b-40{ margin-bottom: 40px; }
      .margin-l-10{ margin-left: 10px; }
      .margin-l-20{ margin-left: 20px; }
      .margin-l-30{ margin-left: 30px; }
      .margin-l-40{ margin-left: 40px; }
      .margin-r-10{ margin-right: 10px; }
      .margin-r-20{ margin-right: 20px; }
      .margin-r-30{ margin-right: 30px; }
      .margin-r-40{ margin-right: 40px; }
  body
    div(class="wrap")
      div(class="page")
        div(class="page-inner")
          div(class="page-head")
            div(class="txt-r")
              div(class="logo") Turing
              div(class="logo-sub") Developer Resume
          div(class="page-body")

            div(class="grid")
              div(class="col w-3-3")
                div(class="col-inner")
                  div(class="box personal-info")
                    div(class="box-head")
                      img(class="icon" src="data:image/png;base64," alt="" width="48" height="48")
                      div(class="line")
                      div(class="box-title") Personal info
                    div(class="box-body")
                      div(class="inline-block")
                        div(class="profile-img-holder")
                          img(class="profile-img" src=userInfo.avatar alt="Sample profile img" width="" height="")
                      div(class="inline-block margin-l-20")
                        div(class="user-fullname") #{userInfo.full_name}
                        div(class="user-title") #{userInfo.role} Engineer with #{userInfo.years_of_experience} years of experience
                        div(class="user-subtitle") #{userInfo.years_of_working_remotely} years of remote work experience
                        div(class="user-address inline-block")
                          img(class="icon" src="data:image/png;base64," alt="" width="16" height="16")
                          span(class="txt") #{userInfo.country}
                        div(class="user-worktime inline-block margin-l-20")
                          img(class="icon" src="data:image/png;base64," alt="" width="16" height="16")
                          span(class="txt") #{userInfo.time_from} - #{userInfo.time_to} Pacific Time

            div(class="grid")
              div(class="col w-3-3")
                div(class="col-inner")
                  div(class="box")
                  div(class="box-head")
                    img(class="icon" src="data:image/png;base64," alt="" width="48" height="48")
                    div(class="line")
                    div(class="box-title") Skills
                  div(class="box-body")
                    table(class="tbl-skills")
                      if userSkills['Front End']
                        tr
                          td(style='width: 28%;')
                            span
                              | Frontend Languages
                          td
                            each skill in userSkills['Front End']
                              span= skill.skill_name + ' [' + skill.score + ' yrs' + '], '
                      if userSkills['UI/UX']
                        tr
                          td(style='width: 28%;')
                            span
                              | UI/UX
                          td
                            each skill in userSkills['UI/UX']
                              span= skill.skill_name + ' [' + skill.score + ' yrs' + '], '
                      if userSkills['Back End']
                        tr
                          td
                            span
                              | Backend Languages
                          td
                            each skill in userSkills['Back End']
                              span= skill.skill_name + ' [' + skill.score + ' yrs' + '], '
                      if userSkills['Mobile']
                        tr
                          td(style='width: 28%;')
                            span
                              | Mobile
                          td
                            each skill in userSkills['Mobile']
                              span= skill.skill_name + ' [' + skill.score + ' yrs' + '], '
                      if userSkills['Database']
                        tr
                          td
                            span
                              | Database
                          td
                            each skill in userSkills['Database']
                              span= skill.skill_name + ' [' + skill.score + ' yrs' + '], '
                      if userSkills['Software']
                        tr
                          td
                            span
                              | Software
                          td
                            each skill in userSkills['Software']
                              span= skill.skill_name + ' [' + skill.score + ' yrs' + '], '
                      
                      if userSkills['DevOps']
                        tr
                          td
                            span
                              | DevOps
                          td
                            each skill in userSkills['DevOps']
                              span= skill.skill_name + ' [' + skill.score + ' yrs' + '], '
                      if userSkills['Cloud Technologies']
                        tr
                          td
                            span
                              | Cloud Technologies
                          td
                            each skill in userSkills['Cloud Technologies']
                              span= skill.skill_name + ' [' + skill.score + ' yrs' + '], '
                      if userSkills['Container Technologies']
                        tr
                          td
                            span
                              | Container Technologies
                          td
                            each skill in userSkills['Container Technologies']
                              span= skill.skill_name + ' [' + skill.score + ' yrs' + '], '
                      if userSkills['Version Control']
                        tr
                          td
                            span
                              | Version Control
                          td
                            each skill in userSkills['Version Control']
                              span= skill.skill_name + ' [' + skill.score + ' yrs' + '], '
                      if userSkills['Project Management']
                        tr
                          td
                            span
                              | Project Management
                          td
                            each skill in userSkills['Project Management']
                              span= skill.skill_name + ' [' + skill.score + ' yrs' + '], '
                      if userSkills['Compiled / multipurpose']
                        tr
                          td
                            span
                              | Compiled / multipurpose
                          td
                            each skill in userSkills['Compiled / multipurpose']
                              span= skill.skill_name + ' [' + skill.score + ' yrs' + '], '
                      if userSkills['Testing']
                        tr
                          td
                            span
                              | Testing
                          td
                            each skill in userSkills['Testing']
                              span= skill.skill_name + ' [' + skill.score + ' yrs' + '], '
                      if userSkills['AI']
                        tr
                          td
                            span
                              | AI
                          td
                            each skill in userSkills['AI']
                              span= skill.skill_name + ' [' + skill.score + ' yrs' + '], '
                      if userSkills['Other']
                        tr
                          td
                            span
                              | Other
                          td
                            each skill in userSkills['Other']
                              span= skill.skill_name + ' [' + skill.score + ' yrs' + '], '

            div(style="margin-bottom: 20px;")

            div(class="grid")
              div(class="col w-3-3")
                div(class="col-inner")
                  div(class="box")
                    div(class="box-head")
                      img(class="icon" src="data:image/png;base64," alt="" width="48" height="48")
                      div(class="line")
                      div(class="box-title") Experience
                    div(class="box-body")
                      each experience in userExperience
                        div(class="box-row")
                          div(class="date-holder")
                            div(class="date-start")
                              div(class="date-month") #{experience.end_month}
                                if experience.end_year !== 0 
                                  div(class="date-year") #{experience.end_year}
                                else 
                                  div(class="date-year") Present
                            div(class="date-separator")
                            div(class="date-end")
                              div(class="date-month") #{experience.start_month}
                              div(class="date-year") #{experience.start_year}
                          div(class="content-holder")
                            div(class="title-holder")
                              div(class="title") #{experience.position}
                              div(class="subtitle") #{experience.company}
                            div(class="details-holder")
                              ul
                                each detail in experience.details
                                  li
                                    | #{detail}

            div(class="grid")
              div(class="col w-3-3")
                div(class="col-inner")
                  div(class="box")
                    div(class="box-head")
                      img(class="icon" src="data:image/png;base64," alt="" width="48" height="48")
                      div(class="line")
                      div(class="box-title") Projects
                    div(class="box-body")
                      each project in userProjects
                        div(class="box-row")
                          div(class="date-holder")
                            div(class="date-start")
                              div(class="date-month") #{project.end_month}
                              if project.end_year !== 0 
                                div(class="date-year") #{project.end_year}
                              else 
                                div(class="date-year") Present
                            div(class="date-separator")
                            div(class="date-end")
                              div(class="date-month") #{project.start_month}
                              div(class="date-year") #{project.start_year}
                          div(class="content-holder")
                            div(class="title-holder")
                              div(class="title") #{project.project_name}
                              div(class="subtitle")
                            div(class="details-holder")
                              ul
                                each detail in project.details
                                  li
                                    | #{detail}

            div(class="grid")
              div(class="col w-3-3")
                div(class="col-inner")
                  div(class="box")
                    div(class="box-head")
                      img(class="icon" src="data:image/png;base64," alt="" width="48" height="48")
                      div(class="line")
                      div(class="box-title") Education
                    div(class="box-body")
                      each education in userEducation
                        div(class="box-row")
                          div(class="date-holder")
                            div(class="date-start")
                              div(class="date-month") #{education.end_month}
                              div(class="date-year") #{education.end_year}
                          div(class="content-holder")
                            div(class="title-holder")
                              div(class="title") #{education.degree}
                              div(class="subtitle") #{education.college}

            div(class="grid")
              div(class="col w-3-3")
                div(class="col-inner")
                  div(class="box")
                    div(class="box-head")
                      img(class="icon" src="data:image/png;base64," alt="" width="48" height="48")
                      div(class="line")
                      div(class="box-title") Certification & Awards
                    div(class="box-body")
                      each certification in userCertifications
                        div(class="box-row")
                          div(class="date-holder")
                            div(class="date-start")
                              div(class="date-year") #{certification.year}
                          div(class="content-holder")
                            div(class="title-holder")
                              div(class="title") #{certification.name}
